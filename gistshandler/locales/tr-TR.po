#: gistshandler\gistshandler.py:29
msgid "Gist not found."
msgstr "Gist bulunamadı."

#: gistshandler\gistshandler.py:35
#, docstring
msgid "A cog to create new Gists and edit/delete existing ones!"
msgstr "Yeni Gist'ler oluşturmak ve mevcut olanları düzenlemek/silmek için bir cog!"

#: gistshandler\gistshandler.py:77
msgid "You need to set up a GitHub token with `{ctx.prefix}set api github token,[TOKEN]` first!"
msgstr "Öncelikle `{ctx.prefix}set api github token,[TOKEN]` ile bir GitHub belirteci ayarlamanız gerekiyor!"

#: gistshandler\view.py:92
msgid "{public.value} is not a recognised boolean option."
msgstr "{public.value} tanınan bir boolean seçeneği değil."

#: gistshandler\view.py:101 gistshandler\view.py:199 gistshandler\view.py:381
msgid "GitHub token is invalid or expired."
msgstr "GitHub belirteci geçersiz veya süresi dolmuş."

#: gistshandler\view.py:109
msgid "Gist `{gist.id}` created."
msgstr "Gist `{gist.id}` oluşturuldu."

#: gistshandler\view.py:203
msgid "You're not allowed to edit this gist."
msgstr "Bu gist'i düzenleme izniniz yok."

#: gistshandler\view.py:209
msgid "Gist `{gist.id}` updated."
msgstr "Gist `{gist.id}` güncellendi."

#: gistshandler\view.py:247
msgid "You are not allowed to use this interaction."
msgstr ""

#: gistshandler\view.py:385
msgid "You're not allowed to delete this gist."
msgstr "Bu gist'i silme izniniz yok."

#: gistshandler\view.py:391
msgid "Gist `{gist.id}` deleted."
msgstr "Gist `{gist.id}` silindi."

