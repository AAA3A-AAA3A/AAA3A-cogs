"        `success`: 3\n"
"        `danger`: 4\n"
"        # Aliases\n"
"        `blurple`: 1\n"
"        `grey`: 2\n"
"        `gray`: 2\n"
"        `green`: 3\n"
"        `red`: 4\n"
"        "
msgstr "Přidat tlačítko role ke zprávě.\n\n"
"        `primary`: 1\n"
"        `secondary`: 2\n"
"        `success`: 3\n"
"        `danger`: 4\n"
"        # Aliasy\n"
"        `blurple`: 1\n"
"        `grey`: 2\n"
