#: rolesbuttons\rolesbuttons.py:308 rolesbuttons\rolesbuttons.py:375
#: rolesbuttons\rolesbuttons.py:439 rolesbuttons\rolesbuttons.py:479
msgid "I have to be the author of the message for the role-button to work."
msgstr "Musím být autorem zprávy pro přidání tlačítka role."

#: rolesbuttons\rolesbuttons.py:321 rolesbuttons\rolesbuttons.py:391
msgid "I don't have sufficient permissions on the channel where the message you specified is located.\n"
"I need the permissions to see the messages in that channel."
msgstr "Nemám dostatečná oprávnění na kanálu, kde se nachází zpráva, kterou jste zadali.\n"
"Potřebuji oprávnění k zobrazení zpráv v tomto kanálu."

#: rolesbuttons\rolesbuttons.py:331
msgid "The emoji you selected seems invalid. Check that it is an emoji. If you have Nitro, you may have used a custom emoji from another server."
msgstr ""

#: rolesbuttons\rolesbuttons.py:341 rolesbuttons\rolesbuttons.py:412
msgid "I can't do more than 25 roles-buttons for one message."
msgstr "Nemohu přiřadit více než 25 rolí tlačítek pro jednu zprávu."

#: rolesbuttons\rolesbuttons.py:369
#, docstring
msgid "Add roles-buttons for a message.\n\n"
"        ```[p]rolesbuttons bulk <message> :reaction1:|@role1 :reaction2:|@role2 :reaction3:|@role3```\n"
"        "
msgstr "Přidejte do zprávy tlačítka rolí.\n\n"
"        ```[p]rolesbuttons bulk <zpráva> :reaction1:|@role1 :reaction2:|@role2 :reaction3:|@role3```\n"
"        "

#: rolesbuttons\rolesbuttons.py:381
msgid "You have not specified any valid role-button."
msgstr "Nezadali jste žádné platné tlačítko rolí."

#: rolesbuttons\rolesbuttons.py:402
msgid "An emoji you selected seems invalid. Check that it is an emoji. If you have Nitro, you may have used a custom emoji from another server."
msgstr ""

#: rolesbuttons\rolesbuttons.py:436
#, docstring
msgid "Remove a role-button for a message."
msgstr "Odebrat tlačítko rolí ze zprávy."

#: rolesbuttons\rolesbuttons.py:446 rolesbuttons\rolesbuttons.py:486
msgid "No role-button is configured for this message."
msgstr "Pro tuto zprávu není nakonfigurováno žádné tlačítko rolí."

#: rolesbuttons\rolesbuttons.py:450
msgid "I wasn't watching for this button on this message."
msgstr "Nesledoval jsem toto tlačítko v této zprávě."

#: rolesbuttons\rolesbuttons.py:476
#, docstring
msgid "Clear all roles-buttons for a message."
msgstr "Vymazat všechna tlačítka rolí ze zprávy."

