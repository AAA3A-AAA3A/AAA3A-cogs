#: rolesbuttons\converters.py:41
msgid "The {role.mention} role is a bot integration role and cannot be assigned or removed."
msgstr "Role {role.mention} je role integrace bota a nemůže být přiřazena nebo odstraněna."

#: rolesbuttons\converters.py:47
msgid "The {role.mention} role is an integration role and cannot be assigned or removed."
msgstr "Role {role.mention} je role integrace bota a nemůže být přiřazena nebo odstraněna."

#: rolesbuttons\converters.py:53
msgid "The {role.mention} role is a premium subscriber role and can only be assigned or removed by Nitro boosting the server."
msgstr "Role {role.mention} je prémiová role předplatitele a může být přiřazena nebo odstraněna pouze pomocí Nitro zvýšení serveru."

#: rolesbuttons\converters.py:59
msgid "The {role.mention} role is higher than my highest role in the discord hierarchy."
msgstr "{role.mention} role je vyšší než má nejvyšší role v Discord hierarchii."

#: rolesbuttons\converters.py:65
msgid "The {role.mention} role is higher than your highest role in the discord hierarchy."
msgstr "{role.mention} role je vyšší než má nejvyšší role v Discord hierarchii."

#: rolesbuttons\converters.py:81
msgid "Emoji Role must be an emoji followed by a role separated by either `;`, `,`, `|`, or `-`."
msgstr "Emoji Role musí být emoji, po níž následuje role oddělená buď `;`, `,`, `|`, nebo `-`."

#: rolesbuttons\rolesbuttons.py:42
#, docstring
msgid "A cog to have roles-buttons!"
msgstr "Cog pro přidávání rolí skrz tlačítka!"

#: rolesbuttons\rolesbuttons.py:111 rolesbuttons\rolesbuttons.py:198
msgid "This message is not in Config."
msgstr ""

#: rolesbuttons\rolesbuttons.py:123 rolesbuttons\rolesbuttons.py:207
msgid "This emoji is not in Config."
msgstr ""

#: rolesbuttons\rolesbuttons.py:130 rolesbuttons\rolesbuttons.py:216
msgid "The role I have to put you in no longer exists. Please notify an administrator of this server."
msgstr "Role kterou vás musím dát již neexistuje. Upozorněte prosím správce tohoto serveru."

#: rolesbuttons\rolesbuttons.py:140
msgid "Role-button of {interaction.message.id} in {interaction.channel.id}."
msgstr "Role tlačítko {interaction.message.id} v {interaction.channel.id}."

#: rolesbuttons\rolesbuttons.py:146 rolesbuttons\rolesbuttons.py:232
msgid "I could not add the {role.mention} ({role.id}) role to you. Please notify an administrator of this server."
msgstr "Nemohu Vám přiřadit roli {role.mention} ({role.id}). Upozorněte prosím správce tohoto serveru."

#: rolesbuttons\rolesbuttons.py:154 rolesbuttons\rolesbuttons.py:240
msgid "You now have the role {role.mention} ({role.id})."
msgstr "Přiřadil jsem ti roli {role.mention} ({role.id})."

#: rolesbuttons\rolesbuttons.py:165 rolesbuttons\rolesbuttons.py:251
msgid "I could not remove the {role.mention} ({role.id}) role to you. Please notify an administrator of this server."
msgstr "Nemohu vám odebrat roli {role.mention} ({role.id}). Upozorněte prosím správce tohoto serveru."

#: rolesbuttons\rolesbuttons.py:173
msgid "I removed the role {role.mention} ({role.id})."
msgstr "Odebral jsem ti roli {role.mention} ({role.id})."

#: rolesbuttons\rolesbuttons.py:226
msgid "Role-button of {inter.message.id} in {channel.id}."
msgstr "Role tlačítko {inter.message.id} v {channel.id}."

#: rolesbuttons\rolesbuttons.py:259
msgid "I did remove the role {role.mention} ({role.id})."
msgstr "Odebral jsem ti roli {role.mention} ({role.id})."

