#: seen\seen.py:25
#, docstring
msgid "A cog to check when a member/role/channel/category/user/guild was last active!"
msgstr "Ein Zahnrad, um zu überprüfen, wann ein Mitglied/Rolle/Kanal/Kategorie/Benutzer/Gilde zuletzt aktiv war!"

#: seen\seen.py:753
msgid "[This message]({message_link}) has been sent by {member_mention}."
msgstr "[Diese Nachricht]({message_link}) wurde von {member_mention} gesendet."

#: seen\seen.py:757
msgid "[This message]({message_link}) has been edited by {member_mention}."
msgstr "[Diese Nachricht]({message_link}) wurde von {member_mention} bearbeitet."

#: seen\seen.py:761
msgid "The reaction {reaction} has been added to [this message]({message_link}) by {member_mention}."
msgstr "Die Reaktion {reaction} wurde [dieser Nachricht] ({message_link}) von {member_mention} hinzugefügt."

#: seen\seen.py:769
msgid "The reaction {reaction} has been removed to the [this message]({message_link}) by {member_mention}."
msgstr "Die Reaktion {reaction} wurde von {member_mention} in die [diese Nachricht] ({message_link}) übernommen."

#: seen\seen.py:866
msgid "**The user {_object.mention} was seen {seen}.**"
msgstr "**Der Benutzer {_object.mention} wurde gesehen {seen}. **"

#: seen\seen.py:874
msgid "**The member {_object.mention} was seen {seen}.**"
msgstr "**Das Mitglied {_object.mention} wurde gesehen {seen}. **"

