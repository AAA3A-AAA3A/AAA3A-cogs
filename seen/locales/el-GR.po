#: seen\seen.py:24
#, docstring
msgid "A cog to check when a member/role/channel/category/user/guild was last active!"
msgstr "Ένα γρανάζι για να ελέγξετε πότε ένα μέλος/ρόλος/κανάλι/κατηγορία/χρήστης/γίλντα ήταν τελευταία φορά ενεργό!"

#: seen\seen.py:931
msgid "[This message]({message_link}) has been sent by {member_mention}."
msgstr "[Αυτό το μήνυμα]({message_link}) εστάλη από {member_mention}."

#: seen\seen.py:935
msgid "[This message]({message_link}) has been edited by {member_mention}."
msgstr "[Αυτό το μήνυμα]({message_link}) έχει επεξεργαστεί από τον {member_mention}."

#: seen\seen.py:939
msgid "The reaction {reaction} has been added to [this message]({message_link}) by {member_mention}."
msgstr "Η αντίδραση {reaction} έχει προστεθεί στο [αυτό το μήνυμα]({message_link}) από το {member_mention}."

#: seen\seen.py:947
msgid "The reaction {reaction} has been removed to the [this message]({message_link}) by {member_mention}."
msgstr "Η αντίδραση {reaction} έχει αφαιρεθεί από το [αυτό το μήνυμα]({message_link}) από το {member_mention}."

#: seen\seen.py:979
msgid "This {object_type} is in the ignored users list (`{prefix}seen ignoreme`)."
msgstr "Αυτό το {object_type} βρίσκεται στη λίστα των αγνοημένων χρηστών (`{prefix}seen ignoreme`)."

#: seen\seen.py:1046
msgid "**The user {_object.mention} was seen {seen}.**"
msgstr "**Ο χρήστης {_object.mention} εθεάθη {seen}.**"

#: seen\seen.py:1054
msgid "**The member {_object.mention} was seen {seen}.**"
msgstr "**Το μέλος {_object.mention} εθεάθη {seen}.**"

#: seen\seen.py:1062
msgid "**The role {_object.mention} was seen {seen}.**"
msgstr "**Ο ρόλος {_object.mention} εθεάθη {seen}.**"

#: seen\seen.py:1070
msgid "**The text channel {_object.mention} was seen {seen}.**"
msgstr "**Το κανάλι κειμένου {_object.mention} εθεάθη {seen}.**"

#: seen\seen.py:1078
msgid "**The category {_object.name} was seen {seen}.**"
msgstr "**Η κατηγορία {_object.name} εθεάθη {seen}.**"

#: seen\seen.py:1086
msgid "**The guild {_object.name} was seen {seen}.**"
msgstr "**Η συντεχνία {_object.name} εθεάθη {seen}.**"

#: seen\seen.py:1234
#, docstring
msgid "Check when a member was last active!"
msgstr "Ελέγξτε πότε ένα μέλος ήταν τελευταία φορά ενεργό!"

#: seen\seen.py:1254
#, docstring
msgid "Check when a role was last active!"
msgstr "Ελέγξτε πότε ένας ρόλος ήταν τελευταία φορά ενεργός!"

#: seen\seen.py:1273
#, docstring
msgid "Check when a channel was last active!"
msgstr "Ελέγξτε πότε ένα κανάλι ήταν τελευταία φορά ενεργό!"

#: seen\seen.py:1280
msgid "You do not have permission to view this channel."
msgstr "Δεν έχετε άδεια για να δείτε αυτό το κανάλι."

#: seen\seen.py:1296
#, docstring
msgid "Check when a category was last active!"
msgstr "Ελέγξτε πότε μια κατηγορία ήταν τελευταία φορά ενεργή!"

#: seen\seen.py:1308
msgid "You do not have permission to view any of the channels in this category."
msgstr "Δεν έχετε άδεια να δείτε κανένα από τα κανάλια αυτής της κατηγορίας."

#: seen\seen.py:1325
#, docstring
msgid "Check when a old member was last active!"
msgstr "Ελέγξτε πότε ένα παλιό μέλος ήταν τελευταία φορά ενεργό!"

#: seen\seen.py:1353
#, docstring
msgid "Check when a guild was last active!"
msgstr "Ελέγξτε πότε μια συντεχνία ήταν τελευταία φορά ενεργή!"

#: seen\seen.py:1373
#, docstring
msgid "Check when a user was last active!"
msgstr "Ελέγξτε πότε ένας χρήστης ήταν τελευταία φορά ενεργός!"

#: seen\seen.py:1392
#, docstring
msgid "Check when a old user was last active!"
msgstr "Ελέγξτε πότε ένας παλιός χρήστης ήταν τελευταία φορά ενεργός!"

#: seen\seen.py:1417
#, docstring
msgid "View a Seen Board for members/roles/channels/categories/guilds/users!\n\n"
"        `bots` is a parameter for `members` and `users`. `include_role` and `exclude_role` are parameters for only `members`.\n"
"        "
msgstr "Δείτε ένα Seen Board για μέλη/ρόλους/κανάλια/κατηγορίες/συντεχνίες/χρήστες!\n\n"
"        Το `bots` είναι μια παράμετρος για τα `members` και `users`. `include_role` και `exclude_role` είναι παράμετροι μόνο για τα `members`.\n"
"        "

#: seen\seen.py:1423
msgid "You're not allowed to view the Seen board for guilds and users."
msgstr "Δεν επιτρέπεται να δείτε το Seen board για συντεχνίες και χρήστες."

#: seen\seen.py:1438
#, docstring
msgid "Get Config data stats."
msgstr "Λήψη στατιστικών δεδομένων διαμόρφωσης."

#: seen\seen.py:1472
#, docstring
msgid "Enable or disable a listener."
msgstr "Ενεργοποίηση ή απενεργοποίηση ενός ακροατή."

#: seen\seen.py:1478
msgid "Listener enabled for this/these type(s)."
msgstr "Ο ακροατής είναι ενεργοποιημένος για αυτόν/αυτούς τους τύπους."

#: seen\seen.py:1480
msgid "Listener disabled for this/these type(s)."
msgstr "Ο ακροατής είναι απενεργοποιημένος για αυτόν/αυτούς τους τύπους."

#: seen\seen.py:1484
#, docstring
msgid "Asking Seen to ignore your actions."
msgstr "Ζητώντας από τον Seen να αγνοήσει τις πράξεις σας."

#: seen\seen.py:1492
msgid "You will no longer be seen by this cog and the data I held on you have been deleted."
msgstr "Δεν θα σας βλέπει πλέον αυτό το γρανάζι και τα δεδομένα που διατηρούσα για εσάς έχουν διαγραφεί."

#: seen\seen.py:1499
msgid "You'll be seen again by this cog."
msgstr "Θα σας ξαναδεί αυτό το γρανάζι."

#: seen\seen.py:1504
#, docstring
msgid "Ignore or unignore a specific user."
msgstr "Αγνοήστε ή καταργήστε την αγνόηση ενός συγκεκριμένου χρήστη."

#: seen\seen.py:1511
msgid "{user.mention} ({user.id}) will no longer be seen by this cog, and their data has been deleted."
msgstr "{user.mention} ({user.id}) δεν θα εμφανίζονται πλέον από αυτό το γρανάζι και τα δεδομένα τους έχουν διαγραφεί."

#: seen\seen.py:1520
msgid "{user.mention} ({user.id}) will be seen again by this cog."
msgstr "{user.mention} ({user.id}) θα εμφανιστεί ξανά από αυτό το γρανάζι."

#: seen\seen.py:1528
#, docstring
msgid "Get an embed for check loop status."
msgstr "Λάβετε μια ενσωμάτωση για την κατάσταση του βρόχου ελέγχου."

#: seen\seen.py:1539
#, docstring
msgid "Purge Config for a specified _type or all."
msgstr "Εκκαθάριση των ρυθμίσεων για ένα συγκεκριμένο _τύπο ή για όλα."

#: seen\seen.py:1546
msgid "All Seen data purged."
msgstr "Όλα τα δεδομένα Seen διαγράφηκαν."

#: seen\seen.py:1558
msgid "Seen data purged for this type."
msgstr "Είδε δεδομένα που εκκαθαρίστηκαν για αυτόν τον τύπο."

#: seen\seen.py:1563
#, docstring
msgid "Migrate Seen from Seen by Aikaterna."
msgstr "Μεταναστεύστε Seen από Seen από Aikaterna."

#: seen\seen.py:1570
msgid "Seen by Aikaterna has no data in this bot."
msgstr "Ο Aikaterna δεν έχει δεδομένα σε αυτό το bot."

#: seen\seen.py:1574
msgid "Seen by Aikaterna use an old/new data schema version and isn't compatible with this cog actually."
msgstr "Το Seen by Aikaterna χρησιμοποιεί μια παλιά/νέα έκδοση σχήματος δεδομένων και δεν είναι συμβατό με αυτό το γρανάζι στην πραγματικότητα."

#: seen\seen.py:1637
msgid "Data successfully migrated from Seen by Aikaterna."
msgstr "Τα δεδομένα μεταφέρθηκαν επιτυχώς από το Seen by Aikaterna."

