#: seen\seen.py:24
#, docstring
msgid "A cog to check when a member/role/channel/category/user/guild was last active!"
msgstr "Un bouton pour vérifier quand un membre/rôle/canal/catégorie/utilisateur/guilde a été actif pour la dernière fois !"

#: seen\seen.py:944
msgid "[This message]({message_link}) has been sent by {member_mention}."
msgstr "[Ce message]({message_link}) a été envoyé par {member_mention}."

#: seen\seen.py:948
msgid "[This message]({message_link}) has been edited by {member_mention}."
msgstr "[Ce message]({message_link}) a été édité par {member_mention}."

#: seen\seen.py:952
msgid "The reaction {reaction} has been added to [this message]({message_link}) by {member_mention}."
msgstr "La réaction {reaction} a été ajoutée à [ce message]({message_link}) par {member_mention}."

#: seen\seen.py:960
msgid "The reaction {reaction} has been removed to the [this message]({message_link}) by {member_mention}."
msgstr "La réaction {reaction} a été supprimée dans [ce message]({message_link}) par {member_mention}."

#: seen\seen.py:992
msgid "This {object_type} is in the ignored users list (`{prefix}seen ignoreme`)."
msgstr "Ce {object_type} est dans la liste des utilisateurs ignorés (`{prefix}seen ignoreme`)."

#: seen\seen.py:1059
msgid "**The user {_object.mention} was seen {seen}.**"
msgstr "**L'utilisateur {_object.mention} a été vu {seen}.**"

#: seen\seen.py:1067
msgid "**The member {_object.mention} was seen {seen}.**"
msgstr "**Le membre {_object.mention} a été vu {seen}.**"

#: seen\seen.py:1075
msgid "**The role {_object.mention} was seen {seen}.**"
msgstr "**Le rôle {_object.mention} a été vu {seen}.**"

#: seen\seen.py:1083
msgid "**The text channel {_object.mention} was seen {seen}.**"
msgstr "**Le canal de texte {_object.mention} a été vu {seen}.**"

#: seen\seen.py:1091
msgid "**The category {_object.name} was seen {seen}.**"
msgstr "**La catégorie {_object.name} a été vue {seen}.**"

#: seen\seen.py:1099
msgid "**The guild {_object.name} was seen {seen}.**"
msgstr "**Le serveur {_object.name} a été vue {seen}.**"

