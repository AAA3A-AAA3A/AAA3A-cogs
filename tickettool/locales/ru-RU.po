msgid "Channel:"
msgstr "Канал:"

#: tickettool\tickettool.py:523
msgid "Closed by:"
msgstr "Закрыто:"

#: tickettool\tickettool.py:531
msgid "Deleted by:"
msgstr "Удалено:"

#: tickettool\tickettool.py:539
msgid "Closed at:"
msgstr "Закрыто в:"

